# Firebase Configuration
# Copy this file to .env and fill in your actual Firebase credentials
# Variables must start with NG_APP_ to be accessible in Angular

NG_APP_FIREBASE_API_KEY=YOUR_FIREBASE_API_KEY
NG_APP_FIREBASE_AUTH_DOMAIN=YOUR_PROJECT.firebaseapp.com
NG_APP_FIREBASE_PROJECT_ID=YOUR_PROJECT_ID
NG_APP_FIREBASE_STORAGE_BUCKET=YOUR_PROJECT.firebasestorage.app
NG_APP_FIREBASE_MESSAGING_SENDER_ID=YOUR_MESSAGING_SENDER_ID
NG_APP_FIREBASE_APP_ID=1:YOUR_APP_ID:web:YOUR_WEB_APP_ID
NG_APP_FIREBASE_MEASUREMENT_ID=G-YOUR_MEASUREMENT_ID
NG_APP_FIREBASE_DATABASE_URL=https://YOUR_PROJECT-default-rtdb.REGION.firebasedatabase.app
